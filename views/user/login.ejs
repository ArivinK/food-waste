<!DOCTYPE html>
<html lang="en">
<%- include ../partials/index_header.ejs %>
<body>
<%- include ../partials/nav.ejs %>
   
   <div class="container" style="margin-bottom: 100px">
      
      <div class="text-center reg_msg">
            <h2 style="color:#808080"><span>Login</span></h2>
       </div><br>
      
       <div class="row signup">
     <form action="/user/login" method="post">

          <div class="row form-group input_group">
              <div class="col-sm-12">
                  <input type="text" name="email" id="login_email" class="form-control" placeholder="Organization's Email">
              </div>
          </div>

          <div class="row form-group input_group">
              <div class="col-sm-12">
                  <input type="password" name="password" id="login_pass" class="form-control" placeholder="Password">
              </div>
          </div>

          <div class="row form-group">
              <div class="col-sm-12">
                  <input type="submit" name="login" id="login" class="form-control btn btn-primary" value="Login">
              </div>
          </div>

     </form>
 </div>
</div>
   </div>
     
<%- include ../partials/footer.ejs %>  
</body>
</html>




 
         